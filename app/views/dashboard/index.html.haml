%div.container-fluid
	%div.row
		%div.col-sm-6
			%h3 General Stats
			%ul
				%li
					Parsed Requests:
					%span#parsedRequests
				%li
					Request Timespan:
					%span#requestTimespan
		%div.col-sm-6
	%div.row
		%div.col-sm-4
			%h3 Status Code Counts
			%div.row
				%div.col-sm-8
					%canvas.chart#statusCodeCountCanvas
				%div.col-sm-4
					%div#statusCodeCountLegend
		%div.col-sm-4
			%h3 Route Hits
			%div.row
				%div.col-sm-12
					%canvas.chart#routeHits
			%div.row
				%div.col-sm-12
					%div#routeHitsLegend
		%div.col-sm-4
			%h3 Process Blockers
			%canvas.chart#processBlockers{:height=>400}
