<table class="table table-striped table-bordered table-hover datatable">
<thead>
	<tr>
		<th>Route</th>
		<th>Hits</th>
		<th>%</th>
		<th>sum</th>
		<th>mean</th>
		<th>min</th>
		<th>max</th>
	</tr>
</thead>
<tbody>
{{#each this}}
	<tr>
		<td class="route-name"><div style="background-color: {{color}}" class="color-bullet img-circle"></div>{{route}}</td>
		<td>{{hits}}</td>
		<td>{{percent}}</td>
		<td>{{sum}}</td>
		<td>{{mean}}</td>
		<td>{{min}}</td>
		<td>{{max}}</td>
	</tr>
{{/each}}
</tbody>
</table>